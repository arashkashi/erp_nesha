<p id="notice"><%= notice %></p>

<h1>Pipe Store Outputs</h1>

<table>
  <thead>
    <tr>
      <th>Pipe type</th>
      <th>Amount</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pipe_store_outputs.each do |pipe_store_output| %>
      <tr>
        <td><%= pipe_store_output.pipe_type_id %></td>
        <td><%= pipe_store_output.amount %></td>
        <td><%= pipe_store_output.user_id %></td>
        <td><%= link_to 'Show', pipe_store_output %></td>
        <td><%= link_to 'Edit', edit_pipe_store_output_path(pipe_store_output) %></td>
        <td><%= link_to 'Destroy', pipe_store_output, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pipe Store Output', new_pipe_store_output_path %>
