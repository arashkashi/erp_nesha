<p id="notice"><%= notice %></p>

<h1>Washer Inputs</h1>

<table>
  <thead>
    <tr>
      <th>Washer type</th>
      <th>Amount</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @washer_inputs.each do |washer_input| %>
      <tr>
        <td><%= washer_input.washer_type_id %></td>
        <td><%= washer_input.amount %></td>
        <td><%= washer_input.user_id %></td>
        <td><%= link_to 'Show', washer_input %></td>
        <td><%= link_to 'Edit', edit_washer_input_path(washer_input) %></td>
        <td><%= link_to 'Destroy', washer_input, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Washer Input', new_washer_input_path %>
